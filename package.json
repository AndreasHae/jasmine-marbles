{
  "name": "jasmine-marbles",
  "version": "0.4.0",
  "module": "index.js",
  "es2015": "es6/index.js",
  "main": "bundles/jasmine-marbles.umd.js",
  "license": "MIT",
  "scripts": {
    "bazel": "bazel",
    "test:unit": "node ./tests.js",
    "test": "nyc yarn run test:unit",
    "precommit": "yarn run prettier",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma all --write \"./**/*.ts\""
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "devDependencies": {
    "@angular/bazel": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/compiler-cli": "7.0.2",
    "@bazel/bazel": "^0.19.0",
    "@bazel/typescript": "^0.20.3",
    "@types/jasmine": "^2.8.0",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.0.14",
    "husky": "^0.14.3",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "nyc": "^10.1.2",
    "prettier": "^1.5.2",
    "rxjs": "^6.3.0",
    "typescript": "3.1.1"
  },
  "peerDependencies": {
    "rxjs": "^6.1.0"
  },
  "dependencies": {
    "lodash": "^4.5.0"
  }
}
